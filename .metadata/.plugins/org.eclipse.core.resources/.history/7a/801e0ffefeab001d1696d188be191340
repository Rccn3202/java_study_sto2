package javaHeight03;

import java.util.HashSet;
import java.util.Iterator;
import java.util.Set;

public class SetEx652 {

	public static void main(String[] args) {
		Set<Animal> animalSet = new HashSet<Animal>();
		animalSet.add(new Animal("°­¾ÆÁö",15));
		animalSet.add(new Animal("°í¾çÀÌ",10));
		animalSet.add(new Animal("Åä³¢",5));
		animalSet.add(new Animal("´ß",3));
		
		Iterator<Animal> it = animalSet.iterator();
		while(it.hasNext()) {
			Animal animalAddr=it.next();
			System.out.println(animalAddr.getKind()+ " "_+animalAddr.getLifespan());
		}
		
	}

}
